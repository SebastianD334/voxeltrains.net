<IfModule mod_rewrite.c>
# Enable Rewrite Engine
RewriteEngine On
RewriteBase /
# Redirect to www.
RewriteCond %{HTTP_HOST} ^voxeltrains.net$
RewriteRule (.*) https://www.voxeltrains.net/$1 [R=301,L] 
</IfModule>